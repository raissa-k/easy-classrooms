	<%if( user.educator ){%>
	<li>
		<a href="/teacher" aria-current="<%= active=='teacher-panel' ? 'page' : 'false' %>" class="focus:outline-none h-full flex items-center justify-between pb-2 gap-2 border-b-2 focus:border-accent hover:border-secondary mx-4 <%= active=='teacher-panel' ? 'border-accent' : 'border-transparent' %>">
			<svg aria-hidden="true" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" class="w-5 h-5">
				<path d="M10.75 16.82A7.462 7.462 0 0115 15.5c.71 0 1.396.098 2.046.282A.75.75 0 0018 15.06v-11a.75.75 0 00-.546-.721A9.006 9.006 0 0015 3a8.963 8.963 0 00-4.25 1.065V16.82zM9.25 4.065A8.963 8.963 0 005 3c-.85 0-1.673.118-2.454.339A.75.75 0 002 4.06v11a.75.75 0 00.954.721A7.506 7.506 0 015 15.5c1.579 0 3.042.487 4.25 1.32V4.065z" />
			  </svg>
			Teacher Panel
		</a>
	</li>
	<% } %>
	<li>
		<a href="/user/profile" aria-current="<%= active=='profile' ? 'page' : 'false' %>" class="focus:outline-none h-full flex items-center justify-between pb-2 gap-2 border-b-2 focus:border-accent hover:border-secondary  mx-4 <%= active=='profile' ? 'border-accent' : 'border-transparent' %>">
			<svg aria-hidden="true" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" class="w-5 h-5">
				<path d="M10 8a3 3 0 100-6 3 3 0 000 6zM3.465 14.493a1.23 1.23 0 00.41 1.412A9.957 9.957 0 0010 18c2.31 0 4.438-.784 6.131-2.1.43-.333.604-.903.408-1.41a7.002 7.002 0 00-13.074.003z" />
			  </svg>						
			User profile
		</a>
	</li>
	<li>
		<a href="/logout" class="focus:outline-none h-full flex items-center justify-between pb-2 gap-2 border-b-2 focus:border-accent hover:border-secondary  mx-4 border-transparent">
			<svg aria-hidden="true" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" class="w-5 h-5">
				<path fill-rule="evenodd" d="M3 4.25A2.25 2.25 0 015.25 2h5.5A2.25 2.25 0 0113 4.25v2a.75.75 0 01-1.5 0v-2a.75.75 0 00-.75-.75h-5.5a.75.75 0 00-.75.75v11.5c0 .414.336.75.75.75h5.5a.75.75 0 00.75-.75v-2a.75.75 0 011.5 0v2A2.25 2.25 0 0110.75 18h-5.5A2.25 2.25 0 013 15.75V4.25z" clip-rule="evenodd" />
